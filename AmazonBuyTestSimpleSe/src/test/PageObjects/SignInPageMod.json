{
  "name": "SignInPage",
  "defaultLocale": "en_US",
  "elements": [
    {
      "name": "emailField",
      "locale": [
        {
          "name": "en_US",
          "locator": "ap_email",
          "AllLocators": [
            "tagName=input",
            "xpath=//*[@id='ap_email']",
            "css=input#ap_email.a-input-text.a-span12.auth-autofocus.auth-required-field",
            "class=a-input-text a-span12 auth-autofocus auth-required-field",
            "name=email",
            "tagId=ap_email"
          ]
        }
      ],
      "wait": {
        "until": "visible",
        "for": 45
      }
    },
    {
      "name": "continueButton",
      "locale": [
        {
          "name": "en_US",
          "locator": "continue",
          "AllLocators": [
            "tagName=input",
            "xpath=//*[@id='continue']",
            "css=input#continue.a-button-input",
            "class=a-button-input",
            "tagId=continue"
          ]
        }
      ],
      "wait": {
        "until": "visible",
        "for": 45
      }
    },
    {
      "name": "passField",
      "locale": [
        {
          "name": "en_US",
          "locator": "xpath=//input[@type='password']"
        }
      ],
      "wait": {
        "until": "visible",
        "for": 45
      }
    },
    {
      "name": "signInButton",
      "locale": [
        {
          "name": "en_US",
          "locator": "xpath=//input[@id='signInSubmit']"
        }
      ],
      "wait": {
        "until": "visible",
        "for": 45
      }
    }
  ]
}